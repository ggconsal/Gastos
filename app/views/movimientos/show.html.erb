<div class="page-header">
  <h3><u>Movimiento</u>: <%= @movimiento.mov_obs %></h3>
</div>

<p id="notice"><%= notice %></p>

<p>
  <strong>Mov fecha:</strong>
  <%= @movimiento.mov_fecha %>
</p>

<p>
  <strong>Mov importe:</strong>
  <%= @movimiento.mov_importe %>
</p>

<p>
  <strong>Mov lote:</strong>
  <%= @movimiento.mov_lote %>
</p>

<p>
  <strong>Subrubro:</strong>
  <%= @movimiento.subrubro.sru_desc %>
</p>

<p>
  <strong>Tipogasto:</strong>
  <%= @movimiento.tipogasto.tga_desc %>
</p>

<p>
  <strong>Tipomov:</strong>
  <%= @movimiento.tipomov.tmo_desc %>
</p>

<p>
  <strong>Formapago:</strong>
  <%= @movimiento.formapago.fpa_desc %>
</p>

<p>
  <strong>Usuario:</strong>
  <%= @movimiento.usuario.usu_nombre %>
</p>

<div class="actions">
  <%= link_to "Modificar", edit_movimiento_path(@movimiento),  :class => "btn btn-primary" %>
  <%= link_to 'Ir Atras', movimientos_path, class: "btn btn-primary" %>
</div>
